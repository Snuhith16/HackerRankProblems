#include <stdio.h>

int main() {
    int n, target, count = 0;
    scanf("%d", &n);

    int a[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    scanf("%d", &target);

    for (int i = 0; i < n; i++) {
        if (a[i] == target) {
            count++;
        }
    }

    printf("%d\n", count);

    return 0;
}
