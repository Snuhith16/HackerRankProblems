#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    if (n > 0) {
        int term = 1;
      printf("<");

        for (int i = 0; i < n; i++) {
            if (i > 0) {
                printf(" ");
            }
            printf("%d", term);
            term *= 2;
        }

        printf(">\n");
    }

    return 0;
}
