    scanf("%d", &num2);
    
    
    scanf("%d", &num3);
    
    if(num1>num2){
        
            
            d=num1;
        
    }
    
    if(num2>num1){
        
        
        
        d=num2;
        
        
    }
     if(num3>d){
        
            
            d=num3;
        
    }
        printf("The largest number is : %d", d);
    
    
