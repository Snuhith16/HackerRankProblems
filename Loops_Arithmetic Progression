#include <stdio.h>

int main() {
    int n, a, d;

   
    scanf("%d", &n);
    
    
    if (n == 0) {
        printf("\n");
        return 0;
    }
    
    scanf("%d %d", &a, &d);

   
    for (int i = 0; i < n; i++) {
        printf("%d ", a + i * d);
       
    }
    
    printf("\n");
    return 0;
}
